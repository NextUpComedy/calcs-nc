    "migrate:down": "cross-env NODE_ENV=development ts-node-dev ./src/database/migrations/migrateDown.ts",
    "migrate:reset": "cross-env NODE_ENV=development ts-node-dev ./src/database/migrations/migrateReset.ts"
  },
